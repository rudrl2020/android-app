<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value=""/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".IntroActivity"/>
        <activity android:name=".MenuActivity"/>
        <activity android:name=".LocationActivity"/>
        <activity android:name=".UpdateActivity"/>
        <activity android:name=".NoticeList"/>
        <activity android:name=".NoticeDetailActivity"/>
        <activity android:name=".MenuContents"/>
        <activity android:name=".SubMenuContents"/>
        <activity android:name=".ThirdMenuContents"/>
        <activity android:name=".ForthMenuContents"/>
        <activity android:name=".FifthMenuContents"/>
        <activity android:name=".SixthMenuContents"/>
        <activity android:name=".SeventhMenuContents"/>
        <activity android:name=".MenuOrder"/>
        <activity android:name=".CartActivity"/>
        <activity android:name=".CouponsActivity"/>
        <activity android:name=".SelectOrderList"/>
        <activity android:name=".CouponFindActivity"/>


        <service android:name=".FirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <service android:name=".FirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>

            </intent-filter>

        </service>
    </application>

</manifest>